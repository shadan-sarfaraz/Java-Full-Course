package Session7Shorting;

import java.util.Scanner;

public class LinearSearch {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int []arr = new int [n];
		int found = s.nextInt();
		for (int i = 0; i < n; i++) {
			arr[i]= s.nextInt();
			if (arr[i]==found) {
				
				System.out.println(found+"Is available in array");
			}
		}
		
	}
}
