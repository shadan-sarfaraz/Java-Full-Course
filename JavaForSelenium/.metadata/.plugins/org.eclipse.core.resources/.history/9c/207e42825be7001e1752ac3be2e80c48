package Day1;

import java.util.Scanner;

public class CountNumberOfDigits {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println());
		Count(n);
	}

	 static int Count(int n) {
		// TODO Auto-generated method stub
		 int count = 0;
		 while(n!=0) {
			 ++count;
			 n = n/10;
		 }
		return count;
	}
}
